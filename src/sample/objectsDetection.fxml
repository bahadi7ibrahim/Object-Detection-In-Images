    <children>
        <Button fx:id="btn1" layoutX="20.0" layoutY="25.0" mnemonicParsing="false" onAction="#ButtonSelectAction" text="Select Image"></Button>
        <Label fx:id="label" layoutX="120.0" layoutY="25.0" prefHeight="25.0" prefWidth="350.0"/>
        <Button fx:id="btn2" layoutX="200.0" layoutY="100.0" prefWidth="100.0" mnemonicParsing="false" onAction="#ButtonDetectAction" text="Detect Objects"></Button>
    </children>
</Pane>